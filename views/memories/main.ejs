<h1>Your Memories</h1>

<table class="table striped">
    <thead>
        <th>Name</th>
        <th>Date</th>
        <th>City</th>
        <th>State</th>
        <th>country</th>
        <th>Description</th>
    </thead>
    <tbody>
        <% places.forEach(p => { %>
            <tr>
                <td><a href="/memory/<%=p.id %>"><%= p.name %></a></td>
                <td><%= p.date %></td>
                <td><%= p.city %></td>
                <td><%= p.state %></td>
                <td><%= p.country %></td>
                <td><%= p.description%></td>
            </tr>
        <% }) %>
    </tbody>
</table>
    

